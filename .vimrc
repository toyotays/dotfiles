set nocompatible
set incsearch
set wildmenu wildmode=list:full
colorscheme molokai
syntax on
set nohlsearch
set cursorline
"for black bg
highlight Normal ctermbg=black ctermfg=grey
highlight StatusLine term=none cterm=none ctermfg=black ctermbg=grey
highlight CursorLine term=none cterm=none ctermfg=none ctermbg=darkblue
set number
set laststatus=2
set statusline=%F%r%h%=
set tabstop=4

